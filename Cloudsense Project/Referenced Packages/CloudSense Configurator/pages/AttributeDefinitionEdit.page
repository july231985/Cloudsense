						<apex:selectOptions value="{!typeOptions}"/>
						</apex:selectList>

						<apex:inputfield id="cascade_value__c" value="{!cscfga__Attribute_Definition__c.cscfga__Cascade_value__c}" required="false" rendered="{!!isPackage}"></apex:inputfield>
						<apex:outputLabel value=" " rendered="{!isPackage}"></apex:outputLabel>

						<apex:selectList id="data_type__c" value="{!cscfga__Attribute_Definition__c.cscfga__Data_Type__c}" multiselect="false" size="1" label="{!$ObjectType.cscfga__Attribute_Definition__c.fields.cscfga__Data_Type__c.Label}">
							<apex:selectOptions value="{!dataTypeOptions}"/>
						</apex:selectList>

						<apex:inputfield id="lookup_config__c" value="{!cscfga__Attribute_Definition__c.cscfga__Lookup_Config__c}" required="false" rendered="{!!isPackage}"></apex:inputfield>
