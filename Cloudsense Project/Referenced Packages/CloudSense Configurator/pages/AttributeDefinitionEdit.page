						<apex:inputfield id="line_item_sequence__c" value="{!cscfga__Attribute_Definition__c.cscfga__Line_Item_Sequence__c}" required="false" rendered="{!!isPackage}"></apex:inputfield>
						<apex:inputfield id="pattern__c" value="{!cscfga__Attribute_Definition__c.cscfga__Pattern__c}" required="false" rendered="{!!isPackage}"></apex:inputfield>
