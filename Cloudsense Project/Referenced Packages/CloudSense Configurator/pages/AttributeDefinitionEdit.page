						<apex:pageblocksectionitem />
					</apex:pageBlockSection>

					<apex:pageblocksection title="Display Information" showheader="true" columns="2" rendered="{!!isPackage}" id="customDetails2">
						<apex:inputfield id="configuration_screen__c" value="{!cscfga__Attribute_Definition__c.cscfga__Configuration_Screen__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="screen_section__c" value="{!cscfga__Attribute_Definition__c.cscfga__Screen_Section__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="row__c" value="{!cscfga__Attribute_Definition__c.cscfga__Row__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="column__c" value="{!cscfga__Attribute_Definition__c.cscfga__Column__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="column_span__c" value="{!cscfga__Attribute_Definition__c.cscfga__Column_Span__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="column_style__c" value="{!cscfga__Attribute_Definition__c.cscfga__Column_Style__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="text_input_lines__c" value="{!cscfga__Attribute_Definition__c.cscfga__Text_input_lines__c}" required="false" ></apex:inputfield>
						<apex:inputfield id="list_columns__c" value="{!cscfga__Attribute_Definition__c.cscfga__List_Columns__c}" required="false" ></apex:inputfield>
